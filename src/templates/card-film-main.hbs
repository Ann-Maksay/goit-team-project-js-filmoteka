{{#each this}}
<li class="list-item" data-id="{{id}}">
    <div class="film-card" data-id="{{id}}">

        <div class='img-container'><img class="film-img" src="https://image.tmdb.org/t/p/w300{{poster_path}}"
                alt="{{title}}" data-id="{{id}}"></div>
        <h3 class="film-title" data-id="{{id}}">{{title}}</h3>
        <div class="description" data-id="{{id}}">
            <p class="description-title" data-id="{{id}}">{{formattedGeners}}</p>
            <p class="strip description-title" data-id="{{id}}">|</p>
            <p class="description-title" data-id="{{id}}">{{yearRelease}}</p>
        </div>
    </div>
</li>
{{/each}}